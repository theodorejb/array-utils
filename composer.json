{
    "name": "theodorejb/array-utils",
    "description": "Useful functions for working with arrays",
    "license": "MIT",
    "authors": [
        {
            "name": "Theodore Brown",
            "email": "theodorejb@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.16",
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "theodorejb\\ArrayUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "theodorejb\\ArrayUtils\\Test\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "phpstan analyze",
        "test": "phpunit tests"
    },
    "config": {
        "sort-packages": true
    }
}
