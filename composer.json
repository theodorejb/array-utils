{
    "name": "theodorejb/array-utils",
    "description": "Useful functions for working with arrays",
    "license": "MIT",
    "authors": [
        {
            "name": "Theodore Brown",
            "email": "theodorejb@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "psalm/plugin-phpunit": "^0.18.4",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {"theodorejb\\ArrayUtils\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"theodorejb\\ArrayUtils\\Test\\": "test/"}
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyze": "psalm",
        "test": "phpunit"
    }
}
